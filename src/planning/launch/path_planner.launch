<launch>
    <!-- Robot Configuration -->
    <arg name="config" default="[(0, -1)]" />

    <!-- Planning Node -->
    <node name="path_planner"
          pkg="planning"
          type="path_planner.py"
          args="2 1 0"
          output="screen">

        <param name="config" value="$(arg config)" />
    </node>
</launch>
